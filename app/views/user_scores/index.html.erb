<h1>Listing user_scores</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Beaconid</th>
      <th>Total score</th>
      <th>State</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_scores.each do |user_score| %>
      <tr>
        <td><%= user_score.user_id %></td>
        <td><%= user_score.beaconid %></td>
        <td><%= user_score.total_score %></td>
        <td><%= user_score.state %></td>
        <td><%= link_to 'Show', user_score %></td>
        <td><%= link_to 'Edit', edit_user_score_path(user_score) %></td>
        <td><%= link_to 'Destroy', user_score, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User score', new_user_score_path %>
