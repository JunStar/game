<% unless @cards.blank? %>

<table class="table">

  <tr>
    <th>ID</th>
    <th>ibeacon</th>
    <th>title</th>
    <th>sub_title</th>
    <th>desc</th>
    <th>State</th>
    <th>appid</th>
    <th>cardid</th>
    <th>start_date</th>
    <th>end_date</th> 
    <th>Operate</th>
  </tr>

  <% @cards.each do |card| %>
  <tr>
    <td><%= card.id %></td>
    <td><%= card.beacon_name %></td>
    <td><%= card.title %></td>
    <td><%= card.sub_title %></td>
    <td><%= card.desc %></td>
    <td><%= card.state %></td>
    <td><%= card.appid %></td>
    <td><%= card.cardid %></td>
    <td><%= card.start_date %></td>
    <td><%= card.end_date %></td>
    <td><%= link_to "Edit", edit_admin_card_path(card) %> | <%= link_to "delete", admin_card_path(card), :method => :delete, confirm: "确认删除?" %></td>
  </tr>
  <% end %>
</table>
<% end %>
