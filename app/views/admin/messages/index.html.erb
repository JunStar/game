<% unless @messages.blank? %>

<table class="table">

  <tr>
    <th>ID</th>
    <th>ibeacon</th>
    <th>content</th>
    <th>start time</th>
    <th>end time</th>
    <th>count</th>
    <th>State</th>
    <th>Operate</th>
  </tr>

  <% @messages.each do |message| %>
  <tr>
    <td><%= message.id %></td>
    <td><%= message.beacon_name %></td>
    <td><%= message.content %></td>
    <td><%= message.start_time %></td>
    <td><%= message.end_time %></td>
    <td><%= message.count %></td>
    <td><%= message.state %></td>
    <td><%= link_to "Edit", edit_admin_message_path(message) %> | <%= link_to "delete", admin_message_path(message), :method => :delete, confirm: "确认删除?" %></td>
  </tr>
  <% end %>
</table>
<% end %>
