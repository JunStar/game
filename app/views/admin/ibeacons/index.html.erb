<% unless @ibeacons.blank? %>

<table class="table">

  <tr>
    <th>ID</th>
    <th>name</th>
    <th>user</th>
    <th>remark</th>
    <th>State</th>
    <th>url</th>
    <th>provider</th>
    <th>uid</th>
    <th>url</th> 
    <th>Operate</th>
  </tr>

  <% @ibeacons.each do |ibeacon| %>
  <tr>
    <td><%= ibeacon.id %></td>
    <td><%= ibeacon.name %></td>
    <td><%= ibeacon.user_name %></td>
    <td><%= ibeacon.remark %></td>
    <td><%= ibeacon.state %></td>
    <td><%= ibeacon.url %></td>
    <td><%= ibeacon.provider %></td>
    <td><%= ibeacon.uid %></td>
    <td><%= link_to "Edit", edit_admin_ibeacon_path(ibeacon) %> | <%= link_to "delete", admin_ibeacon_path(ibeacon), :method => :delete, confirm: "确认删除?" %></td>
  </tr>
  <% end %>
</table>
<% end %>
