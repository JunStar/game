<% unless @burls.blank? %>

<table class="table">

  <tr>
    <th>ID</th>
    <th>ibeacon</th>
    <th>title</th>
    <th>img</th>
    <th>remark</th>
    <th>State</th>
    <th>url</th>
    <th>pv</th>
    <th>uv</th>
    <th>weight</th> 
    <th>Operate</th>
  </tr>

  <% @burls.each do |burl| %>
  <tr>
    <td><%= burl.id %></td>
    <td><%= burl.beacon_name %></td>
    <td><%= burl.title %></td>
    <td><%= burl.img %></td>
    <td><%= burl.remark %></td>
    <td><%= burl.state %></td>
    <td><%= burl.url %></td>
    <td><%= burl.weight %></td>
    <td><%= burl.pv %></td>
    <td><%= burl.uv %></td>
    <td><%= link_to "Edit", edit_admin_burl_path(burl) %> | <%= link_to "delete", admin_burl_path(burl), :method => :delete, confirm: "确认删除?" %></td>
  </tr>
  <% end %>
</table>
<% end %>
