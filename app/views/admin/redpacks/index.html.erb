<% unless @redpacks.blank? %>

<table class="table">

  <tr>
    <th>ID</th>
    <th>ibeacon</th>
    <th>sender name</th>
    <th>wishing</th>
    <th>action title</th>
    <th>action remark</th>
    <th>min</th>
    <th>max</th>
    <th>suc url/th>
    <th>fail url</th> 
    <th>Operate</th>
  </tr>

  <% @redpacks.each do |redpack| %>
  <tr>
    <td><%= redpack.id %></td>
    <td><%= redpack.beacon_name %></td>
    <td><%= redpack.sender_name %></td>
    <td><%= redpack.wishing %></td>
    <td><%= redpack.action_title %></td>
    <td><%= redpack.action_remark %></td>
    <td><%= redpack.min %></td>
    <td><%= redpack.max %></td>
    <td><%= redpack.suc_url %></td>
    <td><%= redpack.fail_url %></td>
    <td>  <%= link_to '克隆', [:clone,:admin, redpack], data: {confirm: "Are you clone one !"} %> | <%= link_to "Edit", edit_admin_redpack_path(redpack) %> | <%= link_to "delete", admin_redpack_path(redpack), :method => :delete, confirm: "确认删除?" %></td>
  </tr>
  <% end %>
</table>
<% end %>
